#: /home/arkhip0v0/LAD2025/08_I18n/guess.c:19
#, c-format
msgid "Think of a number between 1 and 100.\n"
msgstr "Загадайте число от 1 до 100.\n"

#: /home/arkhip0v0/LAD2025/08_I18n/guess.c:24
#, c-format
msgid "Is your number greater than %d? (y/n): "
msgstr "Ваше число больше, чем %d? (д/н): "

#: /home/arkhip0v0/LAD2025/08_I18n/guess.c:28
#, c-format
msgid "Error: Unexpected end of input (^D).\n"
msgstr "Ошибка: Неожиданный конец ввода (^D).\n"

#: /home/arkhip0v0/LAD2025/08_I18n/guess.c:39
msgid "y"
msgstr "д"

#: /home/arkhip0v0/LAD2025/08_I18n/guess.c:42
msgid "n"
msgstr "н"

#: /home/arkhip0v0/LAD2025/08_I18n/guess.c:46
#, c-format
msgid "Error: Invalid response. Please enter 'y' or 'n'.\n"
msgstr "Ошибка: Неверный ответ. Пожалуйста, введите 'д' или 'н'.\n"

#: /home/arkhip0v0/LAD2025/08_I18n/guess.c:52
#, c-format
msgid "Your number is %d!\n"
msgstr "Ваше число - %d!\n"
